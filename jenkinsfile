pipeline {
    agent any
    stages {
        stage('Lint HTML') {
            steps {
                tidy -q -e *.html
            }
        }
        stage('Upload to AWS') {
            steps {
                withAWS(credentials: 'jenkins', region: 'us-east-2') {
                    s3Upload(file:'index.html', bucket:'jerry-jenkins', path:'index.html')
                }
            }
        }
    }
}